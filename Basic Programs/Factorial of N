import java.io.*;
import java.util.*;

public class Solution {
    
    public static int facttorial(int n){
        int ans=1;
     for(int i=1;i<=n;i++){
         ans*=i;
     }
    return ans;
}
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        
        int ANS = facttorial(n);
        System.out.print(ANS);
    }
}